"etk800_research_electricMotor": {
    "information":{
        "authors":"BeamNG",
        "name":"Research: Electric Motor",
        "value":350,
    }
    "slotType" : "etk_engine",
    "slots": [
        ["type", "default", "description"]
        ["etk800_research_transfercase_electric","etk800_research_transfercase_electric", "Transfer Case"],
    ],
    "flexbodies": [],
    
    "powertrain": [
        ["type", "name", "inputName", "inputIndex"],
